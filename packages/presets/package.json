{
  "name": "@ui-collection-kit/presets",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },

  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "typecheck": "tsc",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@pandacss/types": "^0.53.6",
    "@pandacss/dev": "^0.53.6",
    "effect": "3.10.19"
  },
  "devDependencies": {
    "clean-package": "^2.2.0"
  },
  "peerDependencies": {
    "@pandacss/dev": ">0.53.6"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": ["dist", "src"],
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./src/utils/index.ts",
        "default": "./src/utils/index.ts"
      },
      "require": {
        "types": "./dist/utils/index.d.cts",
        "default": "./dist/utils/index.cjs"
      }
    },
    "./colors/*": {
      "import": {
        "types": "./src/colors/*.ts",
        "default": "./src/colors/*.ts"
      },
      "require": {
        "types": "./dist/colors/*.d.cts",
        "default": "./dist/colors/*.cjs"
      }
    }
  }
}
