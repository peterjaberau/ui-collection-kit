{
  "name": "@ui-collection-kit",
  "private": true,
  "author": "Peter Jaber",
  "license": "MIT",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",

    "demos-ui-blocks": "pnpm --filter \"./demos/ui-blocks\"",
    "start-demos-ui-blocks": "pnpm demos-ui-blocks dev",

    "template-starter": "pnpm --filter \"./templates/starter\"",
    "start-template-starter": "pnpm template-starter dev",
    "template-hr": "pnpm --filter \"./templates/hr\"",
    "start-template-hr": "pnpm template-hr dev",
    "styled-components": "^5.3.6",
    "demos-x-flow": "pnpm --filter \"./demos/x-flow\"",
    "start-demos-x-flow": "pnpm demos-x-flow dev",
    "demos-react-lite": "pnpm --filter \"./demos/react-lite\"",
    "start-demos-react-lite": "pnpm demos-react-lite dev",
    "present-storybook": "pnpm --filter \"./apps/present\"",
    "start-present-storybook": "pnpm present-storybook storybook",
    "packages": "pnpm --filter=./packages/** -r",
    "build": "pnpm packages -- build",
    "postbuild": "pnpm document-types && pnpm document-attr",
    "sync-pkgs": "tsx scripts/sync-packages.ts",
    "clean-pkgs": "pnpm -r exec rm -rf dist .swc *.log",
    "clean": "pnpm clean-pkgs && rm -rf node_modules",
    "nextjs-demo": "pnpm --filter \"./demos/nextjs-demo\"",
    "start-nextjs-demo": "pnpm nextjsdemo dev",
    "unstyled-react": "pnpm --filter \"./packages/unstyled/react\"",
    "start-unstyled-react": "pnpm unstyled-react storybook",
    "generate-machine": "plop machine && pnpm sync-pkgs",
    "generate-util": "plop utility && pnpm sync-pkgs",
    "typecheck": "pnpm packages -- typecheck",
    "prettier": "prettier --check packages/**/*",
    "prettier-fix": "prettier --write packages/**/*",
    "lint": "eslint packages",
    "document-types": "tsx scripts/typedocs.ts",
    "document-attr": "tsx scripts/data-attr.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": "prettier --write"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@types/styled-components": "^5.1.26",
    "@eslint/eslintrc": "3.3.1",
    "@swc/core": "1.11.21",
    "@types/jsdom": "^21.1.7",
    "@types/node": "22.14.1",
    "@types/signale": "1.4.7",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.30.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.5.0",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.6",
    "fast-glob": "3.3.3",
    "find-packages": "10.0.4",
    "node-fetch": "3.3.2",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3",
    "scule": "1.3.0",
    "signale": "1.4.0",
    "ts-morph": "25.0.1",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "vercel-submodules": "1.0.10",
    "vite": "6.3.0",
    "vite-plugin-dts": "4.5.3",
    "vitest": "3.1.1",
    "remark-gfm": "^4.0.1",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/addon-storysource": "^8.6.12",

    "storybook": "8.6.12",
    "@storybook/addon-a11y": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-themes": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-toolbars": "8.6.12",
    "@storybook/addon-viewport": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/nextjs": "^8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@storybook/manager-api": "8.6.12",
    "@storybook/theming": "8.6.12",



    "@storybook/cli": "^8.6.12",
    "@storybook/client-logger": "^8.6.12",
    "@storybook/preview-api": "^8.6.12",
    "@storybook/types": "^8.6.12",


    "@types/react-dom": ">=18.3.1",
    "@types/react": ">=18.3.1",
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "autoprefixer": "^10.4.21",
    "postcss": "^8",
    "@mdx-js/rollup": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.8.1"
}
